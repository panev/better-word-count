{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "compilerOptions": {
    "module": "esnext",
    "baseUrl": ".",
    "types": ["node", "svelte"],
    "inlineSources": true,
    // "module": "ESNext",
    // "target": "es5",
    "allowJs": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "importHelpers": true
    // "lib": ["dom", "es5", "scripthost", "es2015"]
  },
  "include": ["**/*.ts", "**/*.svelte"],
  "exclude": ["node_modules/*"]
}
